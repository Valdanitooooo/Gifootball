---
layout: null
---
[
  {% for post in site.categories["image"] %}
    {
      "featimg"    : "{{ post.featimg | escape }}",
      "videoUrl"    : "{{ post.videoUrl | escape }}",
      "audioUrl"    : "{{ post.audioUrl | escape }}",
      "title"    : "{{ post.title | escape }}",
      "category": "image",
      "tags" 		 : "{{ post.tags | join: ','}}",
      "url"      : "{{ post.url }}",
      "date"     : "{{ post.date | date: '%Y-%m-%d' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
